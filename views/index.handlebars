<div class = "container">
    <nav class="navbar navbar-light bg-light">
  <span class="navbar-brand mb-0 h1">EAT IT</span>
</nav>
<h1> EAT IT</h1>
<br>
<h2> INSERT FOR EAT</h2>
<panel>
    <form class="create-form">
        <div class ="form-group">
            <input type="text" id="newBurger" name="burger">
            <button type="submit" class="btn btn-success" id="addburger">
                <i class="fa fa-fighter-jet"></i>
            </button>
        </div>
    </form>
</panel>

<div class="row">
    <div class ="col-sm-6">
           <div class="panel panel-primary">
      <div class="panel-heading" id="left-panel">IT EAT?</div>
      <div class="panel-body">BURGER EAT
          <ul>
            {{#each burgers}}
            {{#unless devoured}}
              <span>{{this.burger_name}}</span>
              <button type="button" class="eatIt bt btn-warning" id="eatBtn" data-id="{{this.id}}"
              data-newDevoured="{{devour}}"> <i class="fa fa-fighter-jet"></i></button>
            {{/unless}}
            {{/each}}
          </ul>
 
      </div>
    </div> 
    </div>

    <div class ="col-sm-6">
           <div class="panel panel-primary">
      <div class="panel-heading" id="left-panel">IT EAT!</div>
      <div class="panel-body">BURGER GO BYE
          <ul>
            {{#each burgers}}
            {{#if devoured}}
              <span>{{this.burger_name}}</span>
              <button type="button" class="destroyIt bt btn-warning" id="eatBtn" data-id="{{this.id}}"
              data-newDevoured="{{devour}}"> <i class="fa fa-fighter-jet"></i></button>
            {{/if}}
            {{/each}}
          </ul>
 
      </div>
    </div> 
    </div>
</div>
</div>